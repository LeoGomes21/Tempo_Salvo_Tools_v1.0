       <widget class="QPushButton" name="pushButton_converte">
        <property name="styleSheet">
         <string notr="true">/* Estado normal: mantém o estilo original definido pelo Qtdesigner */
QPushButton {
    /* Não há alterações aqui, preserva o visual padrão */
}

/* Ao passar o mouse: aplica o efeito &quot;Windows Vista&quot; com magenta neon,
   onde a parte inferior fica mais clara e iluminada */
QPushButton:hover {
    background: qlineargradient(
        spread: pad,
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0   #ff66ff,  /* Topo levemente mais claro */
        stop: 0.8 #ff00ff,  /* Cor principal: magenta neon */
        stop: 1   #ff99ff   /* Fundo mais claro e iluminado */
    );
    border: 1px solid #ff00ff; /* Realça a borda em magenta neon */
    border-radius: 4px;         /* Garante cantos arredondados, se necessário */
    transition: all 0.2s ease;  /* Suaviza a transição do efeito */
}

/* Ao clicar: fixa o efeito com cor sólida e aparência &quot;pressionada&quot; */
QPushButton:pressed {
    background: qlineargradient(
        spread: pad,
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0   #ff00ff,  /* Gradiente para efeito pressionado */
        stop: 1   #e600e6
    );
    border: 1px solid #b300b3;  /* Borda levemente escurecida */
    transform: scale(0.98);      /* Sutil redução de escala para simular clique */
}
</string>
        </property>
        <property name="text">
         <string>Converte de DXF</string>
        </property>
       </widget>
