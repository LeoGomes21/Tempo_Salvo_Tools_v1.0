                    <property name="styleSheet">
                     <string notr="true">/* Estado normal: mantém o estilo original definido pelo Qtdesigner */
QPushButton {
    /* Não há alterações aqui, preserva o visual padrão */
}

/* Ao passar o mouse: aplica o efeito &quot;Windows Vista&quot; com vermelho neon,
   onde a parte inferior fica mais clara e iluminada */
QPushButton:hover {
    background: qlineargradient(
        spread: pad,
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0   #ff6666,  /* Topo levemente mais claro */
        stop: 0.8 #ff0000,  /* Cor principal: vermelho neon */
        stop: 1   #ff9999   /* Fundo mais claro e iluminado */
    );
    border: 1px solid #ff0000; /* Realça a borda em vermelho neon */
    border-radius: 4px;         /* Garante cantos arredondados, se necessário */
    transition: all 0.2s ease;  /* Suaviza a transição do efeito */
}

/* Ao clicar: fixa o efeito com cor sólida e aparência &quot;pressionada&quot; */
QPushButton:pressed {
    background: qlineargradient(
        spread: pad,
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0   #ff0000,  /* Gradiente para efeito pressionado */
        stop: 1   #e60000
    );
    border: 1px solid #b30000;  /* Borda levemente escurecida */
    transform: scale(0.98);      /* Sutil redução de escala para simular clique */
}
