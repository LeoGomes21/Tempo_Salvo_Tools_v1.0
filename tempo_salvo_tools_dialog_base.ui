    background: qlineargradient(
        spread: pad,
        x1: 0, y1: 0,
        x2: 0, y2: 1,
        stop: 0   #00d9ff,  /* Gradiente para efeito pressionado */
        stop: 1   #00daf7
    );
    border: 1px solid #0099b3;  /* Borda levemente escurecida */
    transform: scale(0.98);      /* Sutil redução de escala para simular clique */
